{
  "name": "jobminis",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy-images": "mkdir -p ./public/assets && cp -r ./assets/img ./public/assets/img",
    "copy-js": "mkdir -p ./public/assets && cp -r ./assets/js ./public/assets/js",
    "watch": "pnpm copy-images && pnpm copy-js && tailwindcss -i ./assets/css/vendor.css -o ./public/assets/css/landing.css -w",
    "build": "pnpm copy-images && pnpm copy-js && tailwindcss -i ./assets/css/vendor.css -o ./public/assets/css/landing.css --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "@moesaid/cleopatra": "^1.1.1",
    "flowbite": "^1.4.7"
  }
}
